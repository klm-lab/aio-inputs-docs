{
  "name": "aio-docs",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run sass && remix build",
    "sass-dev": "sass --watch --no-source-map --style=compressed app/assets/styles/",
    "sass": "sass --no-source-map --style=compressed app/assets/styles/",
    "remix-dev": "remix dev",
    "dev": "concurrently npm:sass-dev npm:remix-dev",
    "start": "netlify serve",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@netlify/functions": "^1.4.0",
    "@react-spring/web": "^9.7.3",
    "@remix-run/css-bundle": "^1.19.0",
    "@remix-run/netlify": "^1.19.0",
    "@remix-run/node": "^1.19.0",
    "@remix-run/react": "^1.19.0",
    "aio-inputs": "^2.0.9",
    "aio-store": "^2.4.4",
    "cross-env": "^7.0.3",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.0",
    "@remix-run/eslint-config": "^1.19.0",
    "@remix-run/serve": "^1.19.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "concurrently": "^8.2.0",
    "eslint": "^8.38.0",
    "sass": "^1.64.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
